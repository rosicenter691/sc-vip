1) clear ; run ;;
2) clear ; reset ;;
3) clear ; autoreboot ;;
4) clear ; gotop ;;
5) clear ; speedtest ;;
6) clear ; xp ;;
7) clear ; Factreset ;;
8) clear ; wget -q https://nata.serv00.net/v1.8/Fls/nameserver && chmod +x nameserver && ./nameserver && reset ;;
9) clear ; nano /etc/banner.txt ;;
10) clear ; change-domain ;;
11) clear ; fixhap ;;
12) clear ; fixcert ;;
13) clear ; fixakunbot ;;
14) clear ; clearcache ;;
15) clear ; clearlog ;;
16) clear ; delet-cache-file ;;
17) clear ; Info_Port ;;
18) clear ; about ;;
19) clear ; limit-on-off ;;
20) clear ; menu ;;
