echo -e " ${z}│$NC [${r}20${NC}]$purple Back to Menu"
echo -e " ${z}│$NC [${r}00${NC}]$purple EXIT "
echo -e " ${z}╰══════════════════════════════════════════════════════════╯${NC}"
echo -e " ${z}╭══════════════════════════════════════════════════════════╮${NC}"
