echo -e " ${z}╰══════════════════════════════════════════════════════════╯${NC}"
echo -e " ${z}╭══════════════════════════════════════════════════════════╮${NC}"
echo -e " ${z}│$NC$r ⇲ $NC$y Telegram ${NC}     $Blue=$NC @FranataaSTORE${NC}"
echo -e " ${z}│$NC$r ⇲ $NC$y WhatsApp ${NC}     $Blue=$NC 6282241967317 $NC"
