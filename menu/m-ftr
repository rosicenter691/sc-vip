echo -e " ${z}│$NC [${r}07${NC}]$purple Factory Reset      "
echo -e " ${z}│$NC [${r}08${NC}]$purple Install SlowDns    "  
echo -e " ${z}│$NC [${r}09${NC}]$purple Change Banner      "  
echo -e " ${z}│$NC [${r}10${NC}]$purple Change Domain      "  
echo -e " ${z}│$NC [${r}11${NC}]$purple Fixx Haproxy       "  
echo -e " ${z}│$NC [${r}12${NC}]$purple Fixx Domain        "  
echo -e " ${z}│$NC [${r}13${NC}]$purple Fix Akun Pada Bot  "  
echo -e " ${z}│$NC [${r}14${NC}]$purple Clear Cache      "
echo -e " ${z}│$NC [${r}15${NC}]$purple Clear Logs       "
echo -e " ${z}│$NC [${r}16${NC}]$purple Clear Cache File "
echo -e " ${z}│$NC [${r}17${NC}]$purple Info Service Port  "
echo -e " ${z}│$NC [${r}18${NC}]$purple Information System "
